@model WorkflowENG.Dal.ViewModels.FlowViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DataModel.SampleForm.Message)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataModel.WorkflowSchemeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataModel.CurrentStateId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataModel.CreateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataModel.ModifiedDate)
        </th>
      
        <th></th>
    </tr>

    @foreach (var item in Model.DataModels)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SampleForm.Message)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkflowSchemeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CurrentStateId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDate)
            </td>
            <td>
                @Html.ActionLink("Move", "CheckForNextStep", new { processId = item.WorkflowInstanceId , id = item.Id }) |
                @Html.ActionLink("Edit", "Edit","SampleForms", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
