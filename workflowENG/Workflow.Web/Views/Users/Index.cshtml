@model WorkflowENG.Dal.ViewModels.UsersViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DataModel.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataModel.Family)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataModel.RoleId)
