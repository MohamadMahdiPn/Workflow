@model WorkflowENG.Dal.ViewModels.TempFlowViewModel
@{
    ViewBag.Title = "StartFlow";
}

<h2>StartFlow</h2>

<form method="post" class="form-horizontal">
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group">
        <label class="control-label col-md-2"></label>
        <div class="col-md-10">
            <select id="SchemeCode" name="SchemeCode" class="form-control">
                <option value="none" selected disabled hidden>انتخاب کنید...</option>
                @foreach (var item in Model.SchemeCodeList)
                {
                    <option value="@item">@item</option>
                }
            </select>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
</form>